{
  "name": "csns-web",
  "version": "1.0.0",
  "description": "CSNS webpage",
  "main": "app/index.tsx",
  "scripts": {
    "test": "npx jest",
    "dev": "webpack-dev-server --config webpack.dev.config.js --hot --host 0.0.0.0 --port 8080 --color",
    "build:prod": "webpack --config webpack.prod.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GenesisSam/csns-web.git"
  },
  "author": "GenesisSam",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/GenesisSam/csns-web/issues"
  },
  "homepage": "https://github.com/GenesisSam/csns-web#readme",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/enzyme": "^3.1.11",
    "@types/jest": "^23.1.3",
    "@types/node": "^10.5.1",
    "@types/normalizr": "^2.0.18",
    "@types/react": "^16.4.5",
    "@types/react-dom": "^16.0.6",
    "@types/react-redux": "^6.0.3",
    "@types/react-router-dom": "^4.2.7",
    "@types/react-router-redux": "^5.0.15",
    "@types/redux-logger": "^3.0.6",
    "@types/redux-thunk": "^2.1.0",
    "autoprefixer": "^8.6.4",
    "awesome-typescript-loader": "^5.2.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-es2015-unicode-regex": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^0.28.11",
    "cssnano": "^3.10.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "jasmine-reporters": "^2.3.1",
    "jest": "^23.2.0",
    "node-sass": "^4.9.0",
    "normalizr": "^3.2.4",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^2.1.5",
    "raf": "^3.4.0",
    "sass-loader": "^7.0.3",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.21.0",
    "svg-inline-loader": "^0.8.0",
    "ts-jest": "^23.0.0",
    "ts-node": "^7.0.0",
    "typescript": "^2.9.2",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.13.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.3"
  },
  "dependencies": {
    "@material-ui/core": "^1.4.0",
    "@types/nock": "^9.3.0",
    "axios": "^0.18.0",
    "history": "^4.7.2",
    "nock": "^9.4.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.7",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "jest": {
    "globals": {
      "NODE_ENV": "test"
    },
    "transform": {
      ".*": "<rootDir>/setup/jestPreprocessor.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/setup/jestReporter.js",
    "verbose": true,
    "rootDir": "",
    "modulePaths": [
      "<rootDir>/",
      "<rootDir>/app"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageDirectory": "output/coverage",
    "coverageReporters": [
      "cobertura"
    ],
    "testRegex": "__tests__/.*.spec.tsx$",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules",
      "<rootDir>/setup",
      "<rootDir>/app/common",
      "<rootDir>/",
      "/__mocks__/",
      "/.*\\.scss$",
      "<rootDir>/app/__tests__",
      "<rootdir>/app/common/icons"
    ],
    "setupFiles": [
      "<rootDir>/app/__tests__/shim.tsx",
      "<rootDir>/app/__tests__/preload.tsx"
    ],
    "automock": true
  }
}
